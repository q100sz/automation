#!/bin/bash

COLOR_DEFAULT="\033[0m"
COLOR_RED="\033[0;31m"
COLOR_GREEN="\033[0;32m"
COLOR_BLUE="\033[0;34m"

printf "${COLOR_RED}\n"

printf "    \$\$\$\$\$\$\$\   \$\$\$\$\$\$\        \$\$\   \$\$\ \$\$\           \$\$\\n"
printf "    \$\$  __\$\$\ \$\$  __\$\$\       \$\$ | \$\$  |\__|          \$\$ |\n"
printf "    \$\$ |  \$\$ |\$\$ /  \__|      \$\$ |\$\$  / \$\$\  \$\$\$\$\$\$\  \$\$ | \$\$\$\$\$\$\$\  \$\$\$\$\$\$\\n"
printf "    \$\$\$\$\$\$\$\ |\\$\$\$\$\$\$\        \$\$\$\$\$  /  \$\$ |\$\$  __\$\$\ \$\$ |\$\$  _____|\$\$  __\$\$\\n"
printf "    \$\$  __\$\$\  \____\$\$\       \$\$  \$\$<   \$\$ |\$\$\$\$\$\$\$\$ |\$\$ |\$\$ /      \$\$\$\$\$\$\$\$ |\n"
printf "    \$\$ |  \$\$ |\$\$\   \$\$ |      \$\$ |\\$\$\  \$\$ |\$\$   ____|\$\$ |\$\$ |      $\$   ____|\n"
printf "    \$\$\$\$\$\$\$  |\\$\$\$\$\$\$  |      \$\$ | \\$\$\ \$\$ |\\$\$\$\$\$\$\$\ \$\$ |\\$\$\$\$\$\$\$\ \\$\$\$\$\$\$\$\\n"
printf "    \_______/  \______/       \__|  \__|\__| \_______|\__| \_______| \_______|\n"

printf "${COLOR_DEFAULT}\n"

